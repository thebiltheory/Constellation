  "manifest_version": 2,

  "name": "Constellation",
  "description": "Get a list of your github starred repo's",
  "version": "1.0",

  "browser_action": {
   "default_icon": "images/icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
